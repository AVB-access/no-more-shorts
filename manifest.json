{
  "manifest_version": 2,
  "name": "No More Shorts",
  "version": "1.0",

  "description": "Redirects all youtube shorts to their normal video url.",

  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "tabs",
    "<all_urls>"
  ],

  "background": {
    "scripts":["no-more-shorts.js"]
  }

}
